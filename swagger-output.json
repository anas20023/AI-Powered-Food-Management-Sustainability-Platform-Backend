      "FoodItem": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1
          },
          "name": {
            "type": "string",
            "example": "Chicken Curry"
          },
          "category": {
            "type": "string",
            "example": "Meat"
          },
          "calories": {
            "type": "number",
            "example": 350
          },
          "unit": {
            "type": "string",
            "example": "kg"
          }
        }
      },
      "InventoryPayload": {
        "type": "object",
        "properties": {
          "user_id": {
            "type": "number",
            "example": 1
          },
          "food_item_id": {
            "type": "number",
            "example": 1
          },
          "quantity": {
            "type": "number",
            "example": 2
          },
          "purchased_date": {
            "type": "string",
            "example": "2025-11-19"
          },
          "expiry_date": {
            "type": "string",
            "example": "2025-12-05"
          },
          "notes": {
            "type": "string",
            "example": "Keep refrigerated"
          },
          "status": {
            "type": "string",
            "example": "available"
          }
        }
      },
      "UploadResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1
          },
          "filename": {
            "type": "string",
            "example": "receipt.jpg"
          },
          "url": {
            "type": "string",
            "example": "/uploads/receipt.jpg"
          },
          "size": {
            "type": "number",
            "example": 34290
          }
        }
      }
    },
